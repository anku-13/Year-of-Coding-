void solver(int a,int b,int c)
{
	// Writ oyur code here
  if((a+b+c)%2==0)
  {
    int x=a+b-c;
    int y=-a+b+c;
    int z=a+c-b;
    if(x<0 || y<0 || z<0)
    
      cout<<"Impossible";
    
    else if(x%2 ||y%2||z%2)
    
      cout<<"Impossible";
    
    else
    
      cout<<x/2<<" "<<y/2<<" "<<z/2;
    
    else
      cout<<"Impossible";
  }
}

